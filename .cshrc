set autolist
set ignoreeof
set noclobber
set history   260

alias vi="vim"
alias v="vim"
alias vm="v ~/.mycshrc"
alias vs="v ~/.cshrc"
alias ss="source ~/.cshrc"
alias vdf="vimdiff"
alias grep='grep --color'
alias lg='ll | grep'

alias ll="ls -la"
alias l="ll"
alias b="cd .."
alias rl="readlink -f"
alias l='ls -lFh --color=auto --group-directories-first'

# Source mycshrc
source ~/.mycshrc

